{
  "name": "cypress-allure-example",
  "version": "1.0.0",
  "description": "Cypress example allure report example against Circle CI",
  "main": "",
  "scripts": {
    "clean": "rm -rf node_modules",
    "reinstall": "npm run clean && npm install",
    "test": "cypress open",
    "test:ci": "DEBUG=cypress:* cypress run --browser chromium --spec cypress/integration/*.spec.js",
    "allure:report": "allure --verbose generate allure-results --clean -o allure-report",
    "allure:clear": "rm -r allure-results/ allure-report cypress/screenshots || true",
    "allure:history": "mv -f allure-report/history allure-results/history && rm -r allure-report || true"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beemi/cypress-allure-example.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/beemi/cypress-allure-example/issues"
  },
  "homepage": "https://github.com/beemi/cypress-allure-example#readme",
  "devDependencies": {
    "@shelex/cypress-allure-plugin": "2.3.6",
    "cypress": "6.6.0"
  }
}
